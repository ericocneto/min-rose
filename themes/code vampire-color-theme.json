{
  "name": "code vampire",
  "colors": {
    "editor.background": "#282a36",
    "editor.foreground": "#f8f8f2",
    "editorLineNumber.foreground": "#6272a4",
    "editorLineNumber.activeForeground": "#f8f8f2",
    "editorCursor.foreground": "#f8f8f2",
    "editor.selectionBackground": "#44475a",
    "editor.inactiveSelectionBackground": "#3e4452",
    "editor.selectionHighlightBackground": "#44475a",
    "editor.lineHighlightBackground": "#2b2d3a",
    "editor.findMatchBackground": "#50fa7b",
    "editor.findMatchHighlightBackground": "#3d5a4a",
    "editorHoverWidget.background": "#21222c",
    "editorWidget.background": "#21222c",
    "editorWidget.border": "#44475a",
    "editorIndentGuide.background": "#2b2d3a",
    "editorIndentGuide.activeBackground": "#44475a",
    "activityBar.background": "#21222c",
    "activityBar.foreground": "#f8f8f2",
    "activityBarBadge.background": "#bd93f9",
    "activityBarBadge.foreground": "#f8f8f2",
    "sideBar.background": "#21222c",
    "sideBar.foreground": "#f8f8f2",
    "sideBarTitle.foreground": "#f8f8f2",
    "sideBarSectionHeader.background": "#282a36",
    "sideBarSectionHeader.foreground": "#f8f8f2",
    "statusBar.background": "#191a21",
    "statusBar.foreground": "#f8f8f2",
    "titleBar.activeBackground": "#191a21",
    "titleBar.activeForeground": "#f8f8f2",
    "panel.background": "#21222c",
    "panel.border": "#191a21",
    "tab.activeBackground": "#282a36",
    "tab.inactiveBackground": "#21222c",
    "tab.activeForeground": "#f8f8f2",
    "tab.inactiveForeground": "#6272a4",
    "terminal.background": "#282a36",
    "terminal.foreground": "#f8f8f2",
    "terminalCursor.foreground": "#f8f8f2",
    "editorError.foreground": "#ff5555",
    "editorWarning.foreground": "#ffb86c",
    "editorInfo.foreground": "#8be9fd",
    "editorHint.foreground": "#50fa7b",
    "notification.background": "#21222c",
    "notification.foreground": "#f8f8f2",
    "dropdown.background": "#21222c",
    "dropdown.foreground": "#f8f8f2",
    "dropdown.border": "#44475a",
    "input.background": "#21222c",
    "input.foreground": "#f8f8f2",
    "input.border": "#44475a",
    "list.activeSelectionBackground": "#44475a",
    "list.activeSelectionForeground": "#f8f8f2",
    "list.inactiveSelectionBackground": "#3e4452",
    "list.inactiveSelectionForeground": "#f8f8f2",
    "list.hoverBackground": "#2b2d3a",
    "list.hoverForeground": "#f8f8f2",
    "peekViewEditor.background": "#282a36",
    "peekViewResult.background": "#21222c",
    "peekViewEditor.matchHighlightBackground": "#3d5a4a",
    "peekViewResult.matchHighlightBackground": "#3d5a4a",
    "gitDecoration.modifiedResourceForeground": "#ffb86c",
    "gitDecoration.addedResourceForeground": "#50fa7b",
    "gitDecoration.deletedResourceForeground": "#ff5555",
    "gitDecoration.untrackedResourceForeground": "#8be9fd"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6272a4"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "constant.other"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Library",
      "scope": [
        "support.other.variable",
        "support.other.namespace",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Diff Removed",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["string.other.link", "markup.underline.link"],
      "settings": {
        "foreground": "#8be9fd",
        "fontStyle": "underline"
      }
    }
  ]
}
